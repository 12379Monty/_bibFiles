	doi = {10.1038/srep19413},
	id = {Yuan2016},
	isbn = {2045-2322},
	journal = {Scientific Reports},
	number = {1},
	pages = {19413},
	title = {Plasma extracellular RNA profiles in healthy and cancer patients},
	ty = {JOUR},
	url = {https://doi.org/10.1038/srep19413},
	volume = {6},
	year = {2016},
	Bdsk-Url-1 = {https://doi.org/10.1038/srep19413}}

