@article{Yuan:2019aa,
	annote = {doi: 10.1111/pan.13576},
	author = {Yuan, Ian and Topjian, Alexis A. and Kurth, Charles D. and Kirschen, Matthew P. and Ward, Christopher G. and Zhang, Bingqing and Mensinger, Janell L.},
