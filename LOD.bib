	da = {2006/05/01},
	date = {2006/05/01},
	date-added = {2021-05-29 17:42:12 -0700},
	date-modified = {2021-05-29 17:42:34 -0700},
	doi = {https://doi.org/10.1897/05-320R.1},
	isbn = {0730-7268},
	journal = {Environmental Toxicology and Chemistry},
