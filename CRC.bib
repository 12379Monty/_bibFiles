	Journal = {PLoS ONE},
	Number = {8},
	Pages = {e103159},
	Publisher = {Public Library of Science},
	Title = {Sporadic Early-Onset Colorectal Cancer Is a Specific Sub-Type of Cancer: A Morphological, Molecular and Genetics Study},
	Ty = {JOUR},
	U1 = {PONE-D-14-12204{$[$}PII{$]$}; 25083765{$[$}pmid{$]$}},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4118858/},
