	dp = {Cambridge University Press},
	editor = {Freedman, David A. and Collier, David and Sekhon, Jasjeet S. and Stark, Philip B.},
	isbn = {9780521195003},
	pages = {337-356},
	publisher = {Cambridge University Press},
	title = {On Types of Scientific Inquiry: The Role of Qualitative Reasoning},
	url = {https://www.cambridge.org/core/product/19AD82B406B3F0B1EAF12B4A6AC33E23},
	year = {2009},
