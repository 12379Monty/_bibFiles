	Month = {09},
	Number = {38},
	Pages = {15377--15382},
	Publisher = {National Academy of Sciences},
	Title = {Drift and conservation of differential exon usage across tissues in primate species},
	Ty = {JOUR},
	U1 = {201307202{$[$}PII{$]$}; 24003148{$[$}pmid{$]$}},
