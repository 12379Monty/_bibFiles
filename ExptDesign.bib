	author = {Tracy, Maureen B. and Cooke, William E. and Gatlin, Christine L. and Cazares, Lisa H. and Weaver, Dennis M. and Semmes, O. John and Tracy, Eugene R. and Manos, Dennis M. and Malyarenko, Dariya I.},
	booktitle = {PROTEOMICS --Clinical Applications},
	da = {2011/08/01},
