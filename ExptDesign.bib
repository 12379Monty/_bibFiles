@article{Yan:2012aa,
	abstract = {Batch effect is one type of variability that is not of primary interest but ubiquitous in sizable genomic experiments. To minimize the impact of batch effects, an ideal experiment design should ensure the even distribution of biological groups and confounding factors across batches. However, due to the practical complications, the availability of the final collection of samples in genomics study might be unbalanced and incomplete, which, without appropriate attention in sample-to-batch allocation, could lead to drastic batch effects. Therefore, it is necessary to develop effective and handy tool to assign collected samples across batches in an appropriate way in order to minimize the impact of batch effects.},
	author = {Yan, Li and Ma, Changxing and Wang, Dan and Hu, Qiang and Qin, Maochun and Conroy, Jeffrey M. and Sucheston, Lara E. and Ambrosone, Christine B. and Johnson, Candace S. and Wang, Jianmin and Liu, Song},
